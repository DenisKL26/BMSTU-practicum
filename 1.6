def Survive(n, k):
    ludi = [0] + [1] * (n - 1)
    i = counter = 0
    kills = 1

    while kills < (n - 1):
        if ludi[i] == 1:
            counter += 1
        if ludi[i] == 1 and counter == k:
            ludi[i] = 0
            counter = 0
            kills += 1
        i += 1
        if i >= n:
            i = 0
    return ludi

    
n = int(input('Введите кол-во людей '))
k = int(input('Очередность убийства '))
print(Survive(n, k))
