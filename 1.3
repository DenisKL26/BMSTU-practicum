def fibsys(x):
    Fib = [1, 2]
    i = 2
    while (Fib[-1] < x): #заполняем массив числами Фиббоначи пока не будет числа больше введенного
        Fib.append(Fib[i-1] + Fib[i-2])
        i += 1
    Fib = Fib[::-1] 
    ans = ''
    for i in range(len(Fib)): #переводим введенное число в ССФ
        if Fib[i] > x:
            ans += '0'
        if Fib[i] <= x:
            ans += '1'
            x -= Fib[i]
    ans = str(int(ans))
    print ("Количество нулей ", ans.count('0'))
    print ('Количество единиц ', ans.count('1'))

x = int(input())
fibsys(x)
